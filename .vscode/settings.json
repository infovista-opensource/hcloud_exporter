{
  "go.delveConfig": {
    "dlvLoadConfig": {
      "followPointers": true,
      "maxVariableRecurse": 1,
      "maxStringLen": 1024,
      "maxArrayValues": 64,
      "maxStructFields": -1
    },
    "apiVersion": 2,
    "showGlobalVariables": true
  },
  // The token will be loaded from the environment
  "terminal.integrated.env.linux": {
    "HCLOUD_TOKEN": "$HCLOUD_TOKEN"
  }
}
